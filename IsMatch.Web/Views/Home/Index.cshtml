<script type="text/html" id="laytplCont">
    {{#  layui.each(d, function(index, item){ }}
    <div class="item">
        <div class="layui-fluid">
            <div class="layui-row">
                <div class="layui-col-xs12 layui-col-sm4 layui-col-md5">
                    <div class="img">
                        {{# if(item.pic){ }}
                        <img src="{{item.pic}}" alt="">
                        {{# } else { }}
                        <img src="~/images/sy_img4.jpg" alt="">
                        {{# } }}
                    </div>
                </div>
                <div class="layui-col-xs12 layui-col-sm8 layui-col-md7">
                    <div class="item-cont">
                        <h3>
                            {{ item.title }}
                            {{# if(item.isNew){ }}
                            <button class="layui-btn layui-btn-danger new-icon">new</button>
                            {{# }else {}}
                            {{# } }}
                        </h3>
                        <h5>{{ item.pName }}</h5>
                        <p>
                            {{ item.keyword }}
                        </p>
                        {{# if(item.enTitle){ }}
                        <a href="/detail/{{ item.enTitle }}" class="go-icon"></a>
                        {{# } else { }}
                        <a href="/detail/{{ item.title }}" class="go-icon"></a>
                        {{# } }}

                    </div>
                </div>
            </div>
        </div>
    </div>
    {{#  }); }}
</script>

@section Scripts{
    <script type="text/javascript">
        layui.config({
